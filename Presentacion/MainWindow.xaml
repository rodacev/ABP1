    </Window.Resources>


    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="aplicacion/imagenes/logo_white_background.jpg" Stretch="Uniform"/>
        </Grid.Background>

        <Button x:Name="btn_agregarCliente" Content="" HorizontalAlignment="Left" Margin="65,84,0,0" VerticalAlignment="Top" Width="84" Height="84" BorderThickness="0" Click="btn_agregarCliente_Click" Style="{DynamicResource clienteAddStyle}" ToolTipService.InitialShowDelay="100">
            <Button.ToolTip>
                <ToolTip Placement="Right" Height="40" Width="120" Margin="20,0,0,0" Background="#1290cb" BorderBrush="Transparent" Foreground="White" PlacementRectangle="-20,100,0,0" HasDropShadow="True" HorizontalContentAlignment="Center">
                    <StackPanel>
                        <TextBlock FontWeight="Bold" FontSize="14">Agregar Cliente</TextBlock>
                    </StackPanel>
                </ToolTip>
            </Button.ToolTip>
            <Button.Background>
                <ImageBrush ImageSource="aplicacion/imagenes/ClientAdd.png" Stretch="Uniform"/>
            </Button.Background>
        </Button>


        <Button x:Name="btn_buscar" Content="" HorizontalAlignment="Left" Margin="65,270,0,0" VerticalAlignment="Top" Width="84" Height="84" BorderThickness="0" Style="{DynamicResource clienteSearchStyle}" Click="Button_Click" ToolTipService.InitialShowDelay="100">
            <Button.ToolTip>
                <ToolTip Placement="Right" Height="40" Width="120" Margin="20,0,0,0" Background="#1290cb" BorderBrush="Transparent" Foreground="White" PlacementRectangle="-20,100,0,0" HasDropShadow="True" HorizontalContentAlignment="Center">
                    <StackPanel>
                        <TextBlock FontWeight="Bold" FontSize="14">Buscar Cliente</TextBlock>
                    </StackPanel>
                </ToolTip>
            </Button.ToolTip>
            <Button.Background>
                <ImageBrush ImageSource="aplicacion/imagenes/ClientSearch.png" Stretch="Uniform"/>
            </Button.Background>
        </Button>


        <Button x:Name="btn_close"  HorizontalAlignment="Left" Margin="770,0,0,0" VerticalAlignment="Top" Width="30" Height="30" BorderThickness="0" Style="{DynamicResource closeStyle}" Click="btn_close_Click" RenderTransformOrigin="0.333,0.733">
            <Button.Background>
                <ImageBrush ImageSource="aplicacion/imagenes/close_default.png" Stretch="Uniform"/>
            </Button.Background>
        </Button>
        <Label HorizontalAlignment="Left" Margin="280,289,0,0" VerticalAlignment="Top" Height="62" Width="239">
            <Label.Background>
                <ImageBrush ImageSource="aplicacion/imagenes/Logo Duoc UC Escuela IT.jpg" Stretch="None"/>
            </Label.Background>
        </Label>
        <Button x:Name="btn_agregarContrato" Content="" HorizontalAlignment="Left" Margin="659,84,0,0" VerticalAlignment="Top" Width="84" Height="84" BorderThickness="0" Style="{DynamicResource ContractAddStyle}" Click="btn_agregarContrato_Click" ToolTipService.InitialShowDelay="100">
            <Button.ToolTip>
                <ToolTip Placement="Right" Height="40" Width="120" Margin="20,0,0,0" Background="#1290cb" BorderBrush="Transparent" Foreground="White" PlacementRectangle="-20,100,0,0" HasDropShadow="True" HorizontalContentAlignment="Center">
                    <StackPanel>
                        <TextBlock FontWeight="Bold" FontSize="14">Crear Contrato</TextBlock>
                    </StackPanel>
                </ToolTip>
            </Button.ToolTip>
            <Button.Background>
                <ImageBrush ImageSource="aplicacion/imagenes/ContractAdd.png" Stretch="Uniform"/>
            </Button.Background>
        </Button>
        <Button x:Name="btn_buscarContrato" Content="" HorizontalAlignment="Left" Margin="659,270,0,0" VerticalAlignment="Top" Width="84" Height="84" BorderThickness="0" Style="{DynamicResource ContractSearchStyle}" Click="btn_buscarContrato_Click" ToolTipService.InitialShowDelay="100">
            <Button.ToolTip>
                <ToolTip Placement="Right" Height="40" Width="120" Margin="20,0,0,0" Background="#1290cb" BorderBrush="Transparent" Foreground="White" PlacementRectangle="-20,100,0,0" HasDropShadow="True" HorizontalContentAlignment="Center">
                    <StackPanel>
                        <TextBlock FontWeight="Bold" FontSize="14">Buscar Contrato</TextBlock>
                    </StackPanel>
                </ToolTip>
            </Button.ToolTip>
            <Button.Background>
                <ImageBrush ImageSource="aplicacion/imagenes/ContractSearch.png" Stretch="Uniform"/>
            </Button.Background>
        </Button>


        <Button x:Name="btn_minus" HorizontalAlignment="Left" Margin="734,0,0,0" VerticalAlignment="Top" Width="31" BorderThickness="0" Height="28" Style="{DynamicResource MinusStyle}" Click="btn_minus_Click">
            <Button.Background>
                <ImageBrush ImageSource="aplicacion/imagenes/minus_default.png" Stretch="Uniform"/>

            </Button.Background>
        </Button>




    </Grid>
</Window>
